{% extends 'base.html.twig' %}

{% block title %}{{ question }}{% endblock %}

{% block body %}

    <div class="container-fluid text-center my-3">

        <h2>{{ question }}</h2><br>

        <h5>{{ questionText|markdown_filter }}</h5>

        {% for q in questions %}
            <p>{{ q }}</p>
        {% endfor %}
    </div>
{% endblock %}